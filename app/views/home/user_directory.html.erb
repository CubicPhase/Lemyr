<div id="UserDirectory">
<%- @users.each do |user| %>

<div class="user_box">
  <h3><%= user.name %></h3>
    <img src="<%= user.avatar_attached.url %>">
    <ul>
       <li> <a href='mailto:<%= user.email %>'>Email</a></li>
       <% user.authentications.each do |auth| %>
        <li> <a href="<%= user.get_profile_for_provider(auth.provider.to_sym) %>" target=_new>
                <%= auth.provider.to_s.gsub(/ oauth2/i,'+').titleize %> 
              </a>
        </li>
       <% end %>
    </ul>
</div>

<% end -%>     

<br style="clear: both;">
</div>